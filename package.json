{
  "name": "cypress-skills-checklist",
  "version": "1.0.0",
  "description": "Checklist para avaliação de habilidades em desenvolvimento de testes automatizados com Cypress.",
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:prod": "cypress open --env environment=prod",
    "test": "cypress run --record false",
    "test:prod": "cypress run --env environment=prod --record false",
    "test:prod:cloud": "cypress run --env environment=prod --record --tag 'prod'"
  },
  "keywords": [
    "cypress",
    "talking-about-testing",
    "test-design-masterclass",
    "test-design-masterclass-tat",
    "testing"
  ],
  "author": "Walmyr <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^14.4.0"
  }
}
